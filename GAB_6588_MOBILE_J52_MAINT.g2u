F.Intrinsic.Control.If(V.DataTable.dtAntennaData.Exists,=,True)
	F.Data.DataTable.Close("dtAntennaData")
F.Intrinsic.Control.EndIf

'F.Intrinsic.String.Build("Select * from GCG_5235_RFID_MAINT where antenna = '{0}' and READER = '{1}'",V.Global.sAntenna, V.Global.sReader, V.Local.sSQL)

F.Intrinsic.String.Build("Select * from GAB_6588_J52_MAINT where TX_CODE = '{0}'","J52", V.Local.sSQL)
F.Data.DataTable.CreateFromSQL("dtAntennaData","Con",V.Local.sSQL,True)
f.Intrinsic.Control.If(v.DataTable.dtAntennaData.RowCount,=,0)
	f.Data.Datatable.AddRow("dtAntennaData","TX_CODE","J52","PRINT_FLAG","False","ENABLED","False","CARTON_LABELS","False")
	F.Data.DataTable.SaveToDB("dtAntennaData","Con","GAB_6588_J52_MAINT","TX_CODE",256,"TX_CODE@!@TX_CODE*!*PRINT_FLAG@!@PRINT_FLAG*!*ENABLED@!@ENABLED*!*CARTON_LABELS@!@CARTON_LABELS")	
f.Intrinsic.Control.Else
	F.Intrinsic.Control.For(V.Local.iCount,0,V.DataTable.dtAntennaData.RowCount--,1)
		V.Local.sTransCode.Set(V.DataTable.dtAntennaData(V.Local.iCount).TX_CODE!fieldVal)
		F.Intrinsic.Control.SelectCase(V.Local.sTransCode)			
			F.Intrinsic.Control.Case("J52")
				F.Intrinsic.Control.If(V.DataTable.dtAntennaData(V.Local.iCount).Enabled!FieldVal,=,"True")
					V.Local.iflag.Set(1)
				F.Intrinsic.Control.Else
					V.Local.iflag.Set(0)
				F.Intrinsic.Control.EndIf
				gui.FMobileJ52Maint.chkJ52.Value(V.Local.iflag)
				gui.FMobileJ52Maint.txtToBinJ52.Text(V.DataTable.dtAntennaData(V.Local.iCount).TOBIN!FieldVal)
				
				F.Intrinsic.Control.If(V.DataTable.dtAntennaData(V.Local.iCount).PRINT_FLAG!FieldVal,=,"True")
					V.Local.iflag.Set(1)
				F.Intrinsic.Control.Else
					V.Local.iflag.Set(0)
				F.Intrinsic.Control.EndIf
				gui.FMobileJ52Maint.chkPrintJ52.Value(V.Local.iflag)
				gui.FMobileJ52Maint.txtPrinterJ52.Text(V.DataTable.dtAntennaData(V.Local.iCount).PRINTER!FieldVal)
				gui.FMobileJ52Maint.txtcopiesJ52.Text(V.DataTable.dtAntennaData(V.Local.iCount).LABEL_COPIES!FieldVal)
	
				
				'Load field mapping
				gui.FMobileJ52Maint.ddlWOfld.Text(V.DataTable.dtAntennaData(V.Local.iCount).FIELD_WO!FieldValTrim)
				gui.FMobileJ52Maint.ddlQtyfld.Text(V.DataTable.dtAntennaData(V.Local.iCount).FIELD_QTY!FieldValTrim)
				gui.FMobileJ52Maint.ddlLotfld.Text(V.DataTable.dtAntennaData(V.Local.iCount).FIELD_LOT!FieldValTrim)
				gui.FMobileJ52Maint.ddlBinfld.Text(V.DataTable.dtAntennaData(V.Local.iCount).FIELD_BIN!FieldValTrim)
				gui.FMobileJ52Maint.ddlHeatfld.Text(V.DataTable.dtAntennaData(V.Local.iCount).FIELD_HEAT!FieldValTrim)
				gui.FMobileJ52Maint.ddlSerialfld.Text(V.DataTable.dtAntennaData(V.Local.iCount).FIELD_SERIAL!FieldValTrim)
				gui.FMobileJ52Maint.ddlUser1fld.Text(V.DataTable.dtAntennaData(V.Local.iCount).FIELD_USER1!FieldValTrim)
				gui.FMobileJ52Maint.ddlUser2fld.Text(V.DataTable.dtAntennaData(V.Local.iCount).FIELD_USER2!FieldValTrim)
				gui.FMobileJ52Maint.ddlUser3fld.Text(V.DataTable.dtAntennaData(V.Local.iCount).FIELD_USER3!FieldValTrim)
				gui.FMobileJ52Maint.ddlUser4fld.Text(V.DataTable.dtAntennaData(V.Local.iCount).FIELD_USER4!FieldValTrim)
				gui.FMobileJ52Maint.ddlUser5fld.Text(V.DataTable.dtAntennaData(V.Local.iCount).FIELD_USER5!FieldValTrim)
				gui.FMobileJ52Maint.ddlUser6fld.Text(V.DataTable.dtAntennaData(V.Local.iCount).FIELD_USER6!FieldValTrim)
				gui.FMobileJ52Maint.ddlUser7fld.Text(V.DataTable.dtAntennaData(V.Local.iCount).FIELD_USER7!FieldValTrim)
				gui.FMobileJ52Maint.ddlUser8fld.Text(V.DataTable.dtAntennaData(V.Local.iCount).FIELD_USER8!FieldValTrim)
				gui.FMobileJ52Maint.ddlUser9fld.Text(V.DataTable.dtAntennaData(V.Local.iCount).FIELD_USER9!FieldValTrim)
				gui.FMobileJ52Maint.ddlmanualclose.Text(V.DataTable.dtAntennaData(V.Local.iCount).FIELD_MANUAL_CLOSE!FieldValTrim)
				'add new mapping field for LABEL_QTY AND CARTON LABELS FLAG
				gui.FMobileJ52Maint.ddllabelqty.Text(V.DataTable.dtAntennaData(V.Local.iCount).FIELD_LABEL_QTY!FieldValTrim)
				gui.FMobileJ52Maint.chkcartonlabels.Value(V.DataTable.dtAntennaData(V.Local.iCount).CARTON_LABELS!FieldValTrim)
				
				F.Intrinsic.String.Split(V.DataTable.dtAntennaData(V.Local.iCount).EXTRA_OPTION!FieldValTrim, "!@#$#@!", V.Local.sExtraOption)
				F.Intrinsic.Control.If(V.Local.sExtraOption(0), =, "DEFAULT")
					gui.FMobileJ52Maint.chkDefaultBin.Value(True)
				F.Intrinsic.Control.ElseIf(V.Local.sExtraOption(0), =, "STOCKING")
					gui.FMobileJ52Maint.chkStockingBin.Value(True)
				F.Intrinsic.Control.EndIf
				
				F.Intrinsic.Control.If(V.Local.sExtraOption.UBound, >, 0)
					gui.FMobileJ52Maint.ddlCloseJ52.Text(V.Local.sExtraOption(1).Trim)
				F.Intrinsic.Control.EndIf
				
				f.Intrinsic.Control.If(V.Screen.FMobileJ52Maint!ddlCloseJ52.Text.Trim,=,"Always Close")
					gui.FMobileJ52Maint.ddlmanualclose.Text("N/A")
					gui.FMobileJ52Maint.ddlmanualclose.Enabled(false)
				f.Intrinsic.Control.Else
					gui.FMobileJ52Maint.ddlmanualclose.Enabled(true)
				f.Intrinsic.Control.EndIf
				
				F.intrinsic.Control.CallSub(ToggleTransactionEnabled, "CONTROLNAME", "chkJ52")
				F.Intrinsic.Control.CallSub(CheckPrinterClicked, "CONTROLNAME", "chkPrintJ52")
				'F.intrinsic.Control.CallSub(chkNotifyJ52_Click)					
				
			F.Intrinsic.Control.CaseElse
				F.Intrinsic.Control.ExitSub
		F.Intrinsic.Control.EndSelect
		
	F.Intrinsic.Control.Next(V.Local.iCount)
f.Intrinsic.Control.EndIf

