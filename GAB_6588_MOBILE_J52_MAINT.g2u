'check if the given import directories exist
'create them if they don't
'pass directories delimited with *!*

F.Intrinsic.Control.Try

V.Local.sDirectories.Declare(String)
V.Local.sPath.Declare(String)
V.Local.bExists.Declare(Boolean)
V.Local.iCounter.Declare(Long)

V.Local.sDirectories.Set(V.Args.Directories)
F.Intrinsic.String.Split(V.Local.sDirectories, "*!*", V.Local.sDirectories)

F.Intrinsic.Control.For(V.Local.iCounter, 0, V.Local.sDirectories.UBound, 1)
	V.Local.sPath.Set(V.Local.sDirectories(V.Local.iCounter))
	F.Intrinsic.File.DirExists(V.Local.sPath, V.Local.bExists)
	F.Intrinsic.Control.If(V.Local.bExists.Not)
		F.Intrinsic.File.CreateDir(V.Local.sPath)
	F.Intrinsic.Control.EndIf
F.Intrinsic.Control.Next(V.Local.iCounter)

F.Intrinsic.Control.Catch
	F.Intrinsic.Control.CallSub(ErrorMessage, "ErrorNumber", V.Ambient.ErrorNumber, "ErrorDescription", V.Ambient.ErrorDescription, "Subroutine", V.Ambient.CurrentSubroutine, "Callback", "FMobileJ52Maint_Unload")
F.Intrinsic.Control.EndTry
Program.Sub.CheckDirectories.End

Program.Sub.LoadOptions.Start
V.Local.sCostMethod.Declare(String)
V.Local.iCostMethod.Declare(Long)
V.Local.bLot.Declare(Boolean, False)
v.Local.sAutoLotMethod.Declare(String)

'lookup WIP2FG partial costing method
F.Global.General.ReadOption(401163, 3, 0, V.Local.iCostMethod)
F.Intrinsic.Control.SelectCase(V.Local.iCostMethod)
	F.Intrinsic.Control.Case(1)
		V.Local.sCostMethod.Set("P-Price")
	F.Intrinsic.Control.Case(2)
		V.Local.sCostMethod.Set("E-Estimate")
	F.Intrinsic.Control.Case(3)
		V.Local.sCostMethod.Set("I-Inventory")
	F.Intrinsic.Control.Case(4)
		V.Local.sCostMethod.Set("A-Actual")
	F.Intrinsic.Control.Case(5)
		V.Local.sCostMethod.Set("R-Remaining")
	F.Intrinsic.Control.Case(6)
		V.Local.sCostMethod.Set("L-Alt Cost")
F.Intrinsic.Control.EndSelect
gui.FMobileJ52Maint.txtclosemethod.Text(V.Local.sCostMethod)

'read the SS > Company Options (Advanced) > Manufacturing > Auto Number Lots with Work Order-Suffix in WIP to FG
F.Global.General.ReadOption(401282, 0, False, V.Local.bLot)
f.Intrinsic.Control.If(v.Local.bLot)
	v.Local.sAutoLotMethod.Set("Adv Mfg-Job-Suffix")
f.Intrinsic.Control.EndIf
'read the SS > Company Options (Advanced) > Inventory > Auto lot PO Receipt and WIP to FG
F.Global.General.ReadOption(400561, 0, False, 0, V.Local.bLot)
f.Intrinsic.Control.If(v.Local.bLot)
	v.Local.sAutoLotMethod.Set("Adv Inv-Auto Lot")
f.Intrinsic.Control.EndIf
gui.FMobileJ52Maint.txtautolot.Text(V.Local.sAutoLotMethod)

Program.Sub.LoadOptions.End
