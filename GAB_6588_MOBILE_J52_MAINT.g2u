Program.Sub.LoadLockNotifications.Start
'pass ANTENNA, READER, TRANSACTION

F.Intrinsic.Control.Try

V.Local.sPath.Declare(String)
V.Local.sContents.Declare(String)
V.Local.bExists.Declare(Boolean)
V.Local.sFileName.Declare(String)

F.Intrinsic.String.Build("{0}-{1}-{2}", V.Args.Antenna, V.Args.Reader, V.Args.Transaction, V.Local.sFileName)
F.Intrinsic.File.MakeFilenameFriendly(V.Local.sFileName, V.Local.sFileName)

F.Intrinsic.String.Build("{0}\Custom\5235\{1}\Work Order Lock Notifications\{2}.dat", V.Caller.GlobalDir, V.Caller.CompanyCode, V.Local.sFileName, V.Local.sPath)
F.Intrinsic.File.Exists(V.Local.sPath, V.Local.bExists)

F.Intrinsic.Control.If(V.Local.bExists)
	F.Intrinsic.File.File2String(V.Local.sPath, V.Local.sContents)
	F.Intrinsic.String.Split(V.Local.sContents, V.Ambient.NewLine, V.Local.sContents)
	F.Intrinsic.Control.If(V.Local.sContents.UBound, =, 2)
		GUI.FormLockNotifications.chkLockNotifications.Value(V.Local.sContents(0))
		GUI.FormLockNotifications.txtSender.Text(V.Local.sContents(1))
		GUI.FormLockNotifications.txtRecipientAddresses.Text(V.Local.sContents(2))
	F.Intrinsic.Control.Else
		GUI.FormLockNotifications.chkLockNotifications.Value(False)
		GUI.FormLockNotifications.txtSender.Text("")
		GUI.FormLockNotifications.txtRecipientAddresses.Text("")
	F.Intrinsic.Control.EndIf
F.Intrinsic.Control.Else
	GUI.FormLockNotifications.chkLockNotifications.Value(False)
	GUI.FormLockNotifications.txtSender.Text("")
	GUI.FormLockNotifications.txtRecipientAddresses.Text("")
F.Intrinsic.Control.EndIf

F.Intrinsic.Control.Catch
	F.Intrinsic.Control.CallSub(ErrorMessage, "ErrorNumber", V.Ambient.ErrorNumber, "ErrorDescription", V.Ambient.ErrorDescription, "Subroutine", V.Ambient.CurrentSubroutine, "Callback", "FMobileJ52Maint_Unload")
F.Intrinsic.Control.EndTry
Program.Sub.LoadLockNotifications.End

